{
  "name": "cat-pun",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "https://github.com/chocpanda/cat-pun",
  "bugs": "https://github.com/ChocPanda/cat-pun/issues",
  "author": "ChocPanda",
  "license": "MIT",
  "private": true,
  "scripts": {
    "commit": "git-cz",
    "build": "rollup -c",
    "test": "ava",
    "test:unit": "ava",
    "test:cov": "nyc ava && nyc report",
    "test:report": "nyc ava --tap | tap-junit --output ./reports/ava --name test-results.xml",
    "lint": "xo",
    "lint:test": "xo",
    "lint:fix": "xo --fix",
    "lint:report": "xo --reporter junit > ./reports/xo/test-results.xml",
    "release": "semantic-release",
    "docs-toc": "node ./scripts/update-toc.js"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "ava": "^1.4.1",
    "ava-env": "^2.0.0",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "2.1.0",
    "lerna": "^3.14.1",
    "markdown-toc": "^1.2.0",
    "nyc": "^14.0.0",
    "semantic-release": "^15.13.3",
    "tap-junit": "^3.1.0",
    "xo": "^0.24.0"
  },
  "xo": {
    "prettier": "true"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
